{
  "name": "lewisjenkins/craft-rrule",
  "description": "RRULE helpers for Craft CMS (wraps rlanvin/php-rrule).",
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "craftcms/cms": "^5.0",
    "rlanvin/php-rrule": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "lewisjenkins\\rrulewrapper\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "lewisjenkins\\rrulewrapper\\RRuleWrapper"
  },
  "keywords": [
    "craftcms",
    "craft",
    "yii2",
    "rrule",
    "recurrence",
    "calendar",
    "scheduling",
    "module"
  ],
  "homepage": "https://github.com/lewisjenkins/craft-rrule",
  "support": {
    "issues": "https://github.com/lewisjenkins/craft-rrule/issues",
    "source": "https://github.com/lewisjenkins/craft-rrule"
  }
}
